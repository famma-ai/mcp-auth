{
    "name": "@famma/mcp-auth",
    "version": "0.0.1",
    "private": false,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
